{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "es5",
      "es2015",
      "es2016",
      "es2017",
      "dom"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "declaration": true,
    "strictNullChecks": true,
    "outDir": "./",
    "baseUrl": "./",
    "paths": {},
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "types": [
    "node"
  ],
  "compileOnSave": false,
  "files": [
    "index.ts"
  ],
  "extends": "",
  "include": [
    "index.ts"
  ]
}